[
    `0 1 0 {1 sprint} for'
    `0 1 5 {1 sprint} for'
    `0 2 5 {1 sprint} for'
    `5 -1 0 {1 sprint} for'
    `5 -2 0 {1 sprint} for'
    `5 -2 -5 {1 sprint} for'
    `0 1 5 {1 sprint exit} for'
]{
    dup 1 sprint
    cvx eval
} foreach
