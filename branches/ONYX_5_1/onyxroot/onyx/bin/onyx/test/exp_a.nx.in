#
# Integer input.
#

`Integer input:\n' print
-5 1 -1 {
    `e^' print dup cvs print `: ' print
    exp <$w 6 $d 2> output `\n' print
} for

0 1 5 {
    ` e^' print dup cvs print `: ' print
    exp <$w 6 $d 2> output `\n' print
} for

#
# Real input.
#

`Real input:\n' print
-50 1 -1 {
    `e^' print dup 10.0 div <$d 2> output `: ' print
    10.0 div exp <$w 6 $d 2> output `\n' print
} for

0 1 50 {
    ` e^' print dup 10.0 div <$d 2> output `: ' print
    10.0 div exp <$w 6 $d 2> output `\n' print
} for
