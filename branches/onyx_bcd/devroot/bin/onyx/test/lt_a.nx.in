`Test begin\n' print flush

[
	% Equal.
	`-1 -1'
	`0 0'
	`1 1'

	% Less than.
	`-2 -1'
	`-1 0'
	`0 1'
	`1 2'
	`-1 1'

	% Greater than.
	`-1 -2'
	`0 -1'
	`1 0'
	`2 1'
	`1 -1'

	% Equal.
	``abc' `abc''

	% Less than.
	``abc' `abd''

	% Greater than.
	``abd' `abc''
]{
	dup 1 sprint
	cvx eval
	lt
	1 sprint
} foreach

`Test end\n' print
