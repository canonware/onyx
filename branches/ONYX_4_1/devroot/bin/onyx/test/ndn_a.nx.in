`Test begin\n' print flush

[
    {}
    {0}
    {1 0}
    {2 1 0}
    {3 2 1 0}
    {4 3 2 1 0}
]{
    $proc exch def
    0 1 $proc load length {
	dup cvs print
	` ndn: ' print
	$proc load 1 sprints print
	` --> ' print

	proc
	adn ndn
	ostack 1 sprint
	clear
    } for
} foreach

`Test end\n' print
pstack
