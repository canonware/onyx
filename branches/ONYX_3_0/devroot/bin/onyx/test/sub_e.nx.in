    `0.0 0 sub'
    `0.0 -1 sub'
    `-1.0 0 sub'
    `1.0 1 sub'
    `-1.0 -1 sub'

    `0 0.0 sub'
    `0 -1.0 sub'
    `-1 0.0 sub'
    `1 1.0 sub'
    `-1 -1.0 sub'

    `0 0.0 sub'
    `0 -1.0 sub'
    `-1 0.0 sub'
    `1 1.0 sub'
