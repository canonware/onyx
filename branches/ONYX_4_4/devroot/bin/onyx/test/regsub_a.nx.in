`Test begin\n' print flush

[
    {`' `'}
    {`.' `'}
    {`.' `' <>}
    {`.' `' <$g true $i true $m true $s true>}
    {`([a-z]+)' `\1\2'}
    {`([a-z]+)' `\1\2' <$g true $i true $m true $s true>}
    {`([a-z]+)' `\\\1\2'}
]{
    dup 1 sprint
    eval regsub
    1 sprint
} foreach

`Test end\n' print
pstack
