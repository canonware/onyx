`Test begin\n' print flush

[
    `0 ncat'
    `[$a] 0 ncat'
    `[] 1 ncat'
    `[$a] 1 ncat'
    `[] [] 2 ncat'
    `[] [$c $d] 2 ncat'
    `[$a $b] [] 2 ncat'
    `[$a $b] [$c $d] 2 ncat'
    `[$a $b] [$c $d] [$e $f] 3 ncat'

    `() 1 ncat'
    `($a) 1 ncat'
    `() () 2 ncat'
    `() ($c $d) 2 ncat'
    `($a $b) () 2 ncat'
    `($a $b) ($c $d) 2 ncat'
    `($a $b) ($c $d) ($e $f) 3 ncat'

    ``' 1 ncat'
    ``a' 1 ncat'
    ``' `' 2 ncat'
    ``' `cd' 2 ncat'
    ``ab' `' 2 ncat'
    ``ab' `cd' 2 ncat'
    ``ab' `cd' `ef' 3 ncat'

]{
    `In:  ' print
    dup 1 sprint
    cvx eval
    `Out: ' print
    pstack clear
    `\n' print
} foreach

`Test end\n' print
pstack
