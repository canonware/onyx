`Test begin\n' print
`slate.nxm' `slate_init' mrequire

% %=buffer= b2s %string
/b2s {
	marker dup marker_copy
	dup 0 /SEEK_END marker_seek pop
	marker_range_get
} def

buffer
dup marker
% %=buffer= %=marker=
dup `a' marker_before_insert
dup `b' marker_after_insert
dup `c' marker_before_insert
dup `d' marker_after_insert
1 index b2s 1 sprint
dup marker_position 1 sprint
clear

`Test end\n' print
pstack
