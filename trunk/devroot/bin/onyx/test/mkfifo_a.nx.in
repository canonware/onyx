`Test begin\n' print flush

$fifo_name argv 2 get `/mkfifo_a' cat def

fifo_name `p' test {
    fifo_name unlink
} if

fifo_name mkfifo
fifo_name `p' test {
    fifo_name unlink
}{
    `fifo does not exist (1)\n' print
} ifelse

fifo_name 8@444 mkfifo
fifo_name `p' test {
    fifo_name unlink
}{
    `fifo does not exist (2)\n' print
} ifelse

`Test end\n' print
pstack
